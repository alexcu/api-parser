<%#
    Redeclare all enum types
 %>
<%
  @enums.each do |enum|
    enum_type_name = enum[:name].to_pascal_case
    enum_name = enum[:name]
%>

    [MethodImpl(MethodImplOptions.AggressiveInlining)]
    private static int __skadapter__to_sklib_<%= enum_name %>(<%= enum_type_name %> v) { return (int)v; }
    [MethodImpl(MethodImplOptions.AggressiveInlining)]
    private static <%= enum_type_name %> __skadapter__to_<%= enum_name %>(int v) { return (<%= enum_type_name %>)v; }
<%
  end # end enums.each
%>
